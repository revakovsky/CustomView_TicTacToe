<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--  Define attributes that we can set to our TicTacToeView  -->
    <declare-styleable name="TicTacToeView">
        <attr name="player1Color" format="color" />
        <attr name="player2Color" format="color" />
        <attr name="gridColor" format="color" />
    </declare-styleable>

    <!--  Define the attribute for the default style of the TicTacToeView  -->
    <attr name="ticTacToeFieldStyle" format="reference" />

</resources>